{
  "name": "pg-proto-parser",
  "version": "1.30.1",
  "description": "The LaunchQL Proto parser",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/launchql/pgsql-parser/tree/master/packages/proto-parser#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchql/pgsql-parser"
  },
  "bugs": {
    "url": "https://github.com/launchql/pgsql-parser/issues"
  },
  "scripts": {
    "copy": "makage assets",
    "clean": "makage clean dist",
    "prepublishOnly": "npm run build",
    "build": "npm run clean && tsc && tsc -p tsconfig.esm.json && npm run copy",
    "build:dev": "npm run clean && tsc --declarationMap && tsc -p tsconfig.esm.json && npm run copy",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate:test-utils": "ts-node ./scripts/generate-test-utils"
  },
  "keywords": [
    "sql",
    "postgres",
    "postgresql",
    "pg",
    "query",
    "ast",
    "proto",
    "parser",
    "database"
  ],
  "dependencies": {
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "7.28.5",
    "@babel/types": "7.28.5",
    "@launchql/protobufjs": "7.2.6",
    "case": "1.6.3",
    "deepmerge": "4.3.1",
    "nested-obj": "^0.1.1",
    "strfy-js": "^3.0.1"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "makage": "^0.1.3",
    "recast": "0.23.11"
  }
}
